{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://music-api.linust.org/:match*"
    },
    {
      "source": "/music-source/api.php",
      "destination": "https://music-api.gdstudio.xyz/api.php"
    }
  ],
  "build": {
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
}
